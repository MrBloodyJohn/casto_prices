{
  "name": "Casto Prices",
  "version": "1.0",
  "description": "Checks prices in different stores for selected product on Castorama site.",
  "background" : {
    "scripts" : ["background.js"]
  },
  "permissions": [
	"activeTab"
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo16.png"
    }
  },
  "manifest_version": 2,
  "icons": { "16": "logo16.png",
             "48": "logo48.png",
             "128": "logo128.png" }
}
